[Unit]
Description=Group service management
After=mosquitto.service rethinkdb.service network-online.target
Wants=network-online.target
PartOf=rethinkdb.service network-online.target

[Service]
ExecStart=/usr/bin/groupservice -c /etc/groupservice/config.json
KillMode=process
PrivateTmp=true

[Install]
WantedBy=multi-user.target
